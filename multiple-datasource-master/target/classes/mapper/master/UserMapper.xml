<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.clang.dao.master.UserDao">



    <select id="findByName" resultType="String" parameterType="java.lang.String">
        select
        username
        from dba_users
        where username = #{userName}
    </select>

</mapper>